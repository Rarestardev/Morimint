<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<!-- Background -->
<item android:id="@android:id/background">
    <shape>
        <corners android:radius="20dp" />
        <solid android:color="#838383" />
        <stroke android:color="@color/white" android:width="0.5dp"/>
    </shape>
</item>

<!-- Progress -->
<item android:id="@android:id/progress">
    <clip>
        <shape>
            <padding android:bottom="1dp" android:top="1dp"/>
            <corners android:radius="20dp"/>
            <solid android:color="@color/progressMaxValue"/>
            <stroke android:width="0.5dp" android:color="@color/white"/>
        </shape>
    </clip>
</item>
</layer-list>